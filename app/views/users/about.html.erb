<div class="container">
  <div class="row">
    <div class="col-10">
      <h1><%= @user.full_name %></h1>
      <h2>Saved matches</h2>
      <% @user.matches.each do |match| %>
        <% if match.saved? %>
          <p><%=  match.recipe.name %></p>
          <%= link_to "view recipe", match_path(match) %>
        <% end %>
      <% end %>
    </div>
  </div>
</div>
